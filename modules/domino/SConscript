Import('env')

# Link against CGAL if present:
if env['CGAL_LIBS']:
    env.Append(LIBS=env['CGAL_LIBS'])
env.Append(CPPDEFINES='_USE_MATH_DEFINES')


env.IMPModuleBuild(author='Keren Lasker', version='SVN',
                   description='DOMINO optimizer',
                   required_modules=['atom', 'core', 'algebra'])
