Import('env')

env.IMPModuleDoc(env.IMPModuleGetDocs(),
                 authors=["Ben Webb", "Daniel Russel"],
                 brief="This module contains functionality for integrating Modeller with IMP.",
                 overview="""This module supports integration of Modeller with IMP. See
