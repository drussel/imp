                 publications=[env.IMPPublication(authors=["Andrej Sali", "Tom Blundell"],
                                               title="Comparative protein modelling by satisfaction of spatial restraints",
                                               journal="Journal of Molecular Biology",
                                               year="1993")],
                 license=env.IMPStandardLicense() + " Modeller itself is free for academic users. See \\external{http://salilab.org/modeller/, the Modeller page} for more information about licensing Modeller.")
