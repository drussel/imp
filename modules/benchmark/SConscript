Import('env')


module_env=env.IMPModuleBuild(version="SVN",
                   required_modules=['misc', 'domino2', 'atom', 'em', 'core', 'algebra'],
                   optional_dependencies=['CGAL'])

if env['build']=='fast':
    module_env.Alias('benchmark-test', module_env.IMPRun(target="benchmarks.passed",
                                                         source=["#/tools/imppy.sh"]+module_env['IMP_MODULE_BINS']))
    module_env.AlwaysBuild('benchmarks.passed')
