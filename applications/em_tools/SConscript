Import('env')


env= env.IMPApplication(name="em_tools", version='nightly-31/5/2012',
                        authors=["Keren Lasker"],
                        brief="A variety of tools to manipulate density maps",
                        overview="",
                        license=env.IMPStandardLicense(),
                        publications=[],
                        required_modules=['em'],
                        required_dependencies=[])
if env:
    env.IMPPythonExecutable('resample_density.py')
    env.IMPPythonExecutable('mol2pca.py')
    env.IMPPythonExecutable('map2pca.py')
    env.IMPPythonExecutable('simulate_density_from_pdb.py')
